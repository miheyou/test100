#include <stdio.h>
#include <stdlib.h>

void main()
{
    int a[]={1,3,4,5};
    int c=0,e=0,r=0,g=1;
    for(c=0;c<4;c++)
    {
     
      for(e=0;e<4;e++)
      {
          
          if(c!=e)
          {
            
              for(r=0;r<4;r++)
              {
                 if((c!=r)&&(e!=r))
                 {
                    printf("第一个数%d ",a[c]);
                    printf("第二个数%d ",a[e]);
                    printf("第三个数%d ",a[r]);
                    printf("第%d种可能\n",g);
                    g++;
                 }
            
              }
           
          }
          
      }
    
    }
}
